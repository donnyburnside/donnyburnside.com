{
  "name": "donnyburnside.com",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently --kill-others \"npx @11ty/eleventy --config=eleventy.config.js --serve\" \"webpack --stats=minimal --watch\"",
    "build": "cross-env ELEVENTY_ENV=production npx @11ty/eleventy --config=eleventy.config.js",
    "postbuild": "cross-env NODE_ENV=production webpack --progress --stats=minimal",
    "clean": "rimraf ./dist"
  },
  "browserslist": [
    "last 3 chrome versions",
    "last 3 chromeandroid versions",
    "last 3 firefox versions",
    "last 3 opera versions",
    "last 3 edge versions",
    "safari >= 10",
    "ios >= 10"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.2.1",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.22.8",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "luxon": "^2.4.0",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss-loader": "^7.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.52.3",
    "sass-loader": "^13.0.0",
    "tailwindcss": "^3.1.1",
    "terser-webpack-plugin": "^5.3.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donnyburnside/donnyburnside.com.git"
  },
  "author": {
    "name": "Donny Burnside",
    "url": "http://www.donnyburnside.com"
  },
  "homepage": "http://www.donnyburnside.com",
  "license": "UNLICENSED"
}
